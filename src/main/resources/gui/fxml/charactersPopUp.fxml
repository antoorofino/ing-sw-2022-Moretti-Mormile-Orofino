    <HBox fx:id="cardsHBox" alignment="CENTER" fillHeight="false" prefHeight="90.0" spacing="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    <HBox alignment="CENTER" prefHeight="90.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <Button fx:id="buyCardButton" mnemonicParsing="false" prefWidth="150.0" styleClass="btn-orange" text="BUY CARD">
            <font>
                <Font name="Arial Bold" size="15.0" />
            </font>
        </Button>
    </HBox>
</AnchorPane>
