<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="pane" prefHeight="40.0" prefWidth="40.0" stylesheets="@../css/stylesheet.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.client.gui.controllers.FullscreenButtonController">
    <ImageView fx:id="buttonEnter" pickOnBounds="true" preserveRatio="true" styleClass="image-btn">
        <Image url="@../images/fullscreen_enter_icon.png" />
    </ImageView>
    <ImageView fx:id="buttonExit" pickOnBounds="true" preserveRatio="true" styleClass="image-btn" visible="false">
        <Image url="@../images/fullscreen_exit_icon.png" />
    </ImageView>
</StackPane>
